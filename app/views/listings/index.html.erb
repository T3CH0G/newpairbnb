<h1>Listings#index</h1>
<% @listings.each do|x| %>
<%= x.id %> <br>
Title:
<%=x.title%><br>
Description:
<%=x.description%><br>
<p>
Tags: <%= x.tag_list%><br>
</p>
<%if current_user.id == x.user_id%>
<%= link_to "Edit", edit_listing_path(x) %><br>
<%= link_to "Delete", listing_path(x), method: :delete %><br>
<%end%>
<br><br>
<% end %>

<%= link_to "Home", root_path %>



